    <v:String x:Key="DeleteCaption">Видалення</v:String>
    <v:String x:Key="DeleteLesson">Видалити заняття?</v:String>

    <!--Errors-->
    <v:String x:Key="AuthFailcaption">Авторизація</v:String>
