<Page x:Class="SmartTimetable.Pages.Stuff"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SmartTimetable.Pages" 
      xmlns:viewmodels="clr-namespace:SmartTimetable.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Stuff">
    <Page.DataContext>
        <viewmodels:StuffPageVM/>
    </Page.DataContext>
    <Grid>
        <DockPanel>
        <DataGrid DockPanel.Dock="Top" Background="White" Name="DG_Timetable" AutoGenerateColumns="True" ItemsSource="{Binding ts}" Height="394">
            
            
        </DataGrid>
        <Button Content="hello" DockPanel.Dock="Bottom" Width="100" Height="50" Command="{Binding testClick}"></Button>
        </DockPanel>
    </Grid>
</Page>
